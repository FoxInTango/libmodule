PREPARE_TARGETS += prepare.libmodel
PREPARE_URL_libmodel = wf@allinone.io:alpine/libmodel.git
PREPARE_DIR_libmodel = $(DEPENDS_LIBRARY_PATH)/$(basename $(notdir $(PREPARE_URL_libmodel)))
prepare.libmodel:
	-git clone $(PREPARE_URL_libmodel) $(PREPARE_DIR_libmodel)
	-echo "SUPER_MAKE_DIR=${MAKE_FILE_DIR}"                >> $(PREPARE_DIR_libmodel)/.make/super
	-echo "SUPER_MAKE_CONFIG_DIR=${MAKE_CONFIG_DIR}"       >> $(PREPARE_DIR_libmodel)/.make/super
	-cd $(PREPARE_DIR_libmodel) && make prepare
	-rm $(PREPARE_DIR_libmodel)/.make/super
